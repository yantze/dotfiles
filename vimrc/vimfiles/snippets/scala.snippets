snippet /       单行注释
	//  ${0}
snippet //      多行注释
	/* ${0}*/
snippet -       分隔注释
	// -------------------------------${0}
# 类、方法
snippet ob      Object
	object ${1:ob_name} {
	  ${0}
	}
snippet main    主函数
	def main (args: Array[String]) {
	  ${0}
	}
snippet df      函数定义
	def ${1:f_name} (${2}) {
	  ${0}
	}
# 输入输出
snippet pl      输出 [换行]
	println(${0})
