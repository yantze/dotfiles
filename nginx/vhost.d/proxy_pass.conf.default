server {
    listen     80;

    server_name  mirror.vastiny.com;

    access_log logs/mirror.vastiny.com.access.log main;
    error_log  logs/mirror.vastiny.com.error.log;

    # deny all subversions hub
    #
    location ~ /\.(ht|svn|git) {
        deny all;
    }

    location /
    {
      proxy_pass http://127.0.0.1:81; #转向服务
    }
}

