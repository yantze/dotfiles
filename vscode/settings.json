// Place your settings in this file to overwrite the default settings
{
    "workbench.colorTheme": "Solarized Dark",
    "workbench.editor.enablePreviewFromQuickOpen": true,
    "workbench.activityBar.visible": true,
    "window.zoomLevel": 0,
    "files.exclude": {
        "**/*.pyc": true,
        "**/__pycache__": true,
        "**/.*.un~": true,
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "files.eol": "\n",
    "files.trimTrailingWhitespace": true,
    "vim.leader": ",",
    "vim.disableExtension": false,
    "vim.hlsearch": true,
    "vim.useSystemClipboard": true,
    "vim.easymotion": true,
    "vim.foldfix": true,
    // "vim.sneak": true,
    // "vim.sneakReplacesF": true,
    // "vim.sneakUseIgnorecaseAndSmartcase": true
    "vim.insertModeKeyBindings": [],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                ";"
            ],
            "commands": [
                "vim.showQuickpickCmdLine"
            ]
        },
        {
            "before": [
                "j"
            ],
            "after": [
                "g",
                "j"
            ]
        },
        {
            "before": [
                "k"
            ],
            "after": [
                "g",
                "k"
            ]
        },
        {
            "before": [
                "<space>"
            ],
            "after": [
                "z",
                "o"
            ]
        },
        {
            "before": [
                "<leader>",
                "q"
            ],
            "commands": [
                "workbench.action.closeActiveEditor"
            ]
        },
        {
            "before": [
                "<leader>",
                "t",
                "l"
            ],
            "commands": [
                "workbench.action.terminal.toggleTerminal"
            ]
        }
    ],
    "gitlens.advanced.messages": {
        "suppressCommitHasNoPreviousCommitWarning": false,
        "suppressCommitNotFoundWarning": false,
        "suppressFileNotUnderSourceControlWarning": true,
        "suppressGitVersionWarning": false,
        "suppressLineUncommittedWarning": false,
        "suppressNoRepositoryWarning": false,
        "suppressResultsExplorerNotice": false,
        "suppressShowKeyBindingsNotice": true,
        "suppressUpdateNotice": true,
        "suppressWelcomeNotice": true
    },
    "gitlens.historyExplorer.enabled": true,
    "gitlens.gitExplorer.files.layout": "auto",
    "editor.lineNumbers": "relative",
    // 编辑窗口右边缩略图
    "editor.minimap.enabled": false,
    "explorer.confirmDelete": false,
    "editor.fontFamily": "Menlo, Monaco, 'Monaco for Powerline', 'Courier New', monospace",
    "diffEditor.ignoreTrimWhitespace": true,
    "extensions.ignoreRecommendations": true,
    "emmet.showSuggestionsAsSnippets": true,
    "editor.snippetSuggestions": "top",
    // 自动完成时，自动添加双括号
    // "python.autoComplete.addBrackets": true,
    "editor.tabCompletion": "on",
    "editor.renderWhitespace": "none",
    "javascript.updateImportsOnFileMove.enabled": "always", //让vue中的js按编辑器自带的ts格式进行格式化
    "vetur.format.defaultFormatterOptions": {
        "js-beautify-html": {
            "wrap_attributes": "force-aligned" //属性强制折行对齐
        }
    },
    "prettier.semi": false,
    "prettier.trailingComma": "all",
    "prettier.tabWidth": 2,
    "prettier.singleQuote": true,
    // "eslint.autoFixOnSave": true,
    // "tslint.autoFixOnSave": true,
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.formatOnType": false,
    "formattingToggle.affects": [
        // "formatOnPaste",
        "formatOnSave",
        // "formatOnType"
    ],
    "vetur.format.defaultFormatter.js": "vscode-typescript",
    "vetur.format.defaultFormatter.html": "prettier",
    // "editor.codeActionsOnSave": {
    //     "source.fixAll.eslint": true
    // },
    "gitlens.views.repositories.files.layout": "auto",
    "gitlens.views.fileHistory.enabled": true,
    "gitlens.views.lineHistory.enabled": true,
    "cSpell.userWords": [
        "LSTM",
        "NAPI",
        "Powerline",
        "caixin",
        "callout",
        "datetime",
        "devtools",
        "doc",
        "docx",
        "downlevel",
        "dropdown",
        "editor",
        "es",
        "esnext",
        "filepath",
        "fileutils",
        "foldenable",
        "foldfix",
        "foldlevel",
        "foldlevelstart",
        "foldmarker",
        "fullscreenable",
        "gitlens",
        "googleplay",
        "hljs",
        "hlsearch",
        "interop",
        "itunes",
        "keras",
        "leetcode",
        "menlo",
        "mixins",
        "mode",
        "module",
        "monospace",
        "multiline",
        "nodebuffer",
        "numpy",
        "nums",
        "outliner",
        "parens",
        "plotly",
        "promisify",
        "pycache",
        "pymysql",
        "querystring",
        "quickpick",
        "quotemark",
        "resaving",
        "resizable",
        "scripthost",
        "showcmd",
        "singleline",
        "smartcase",
        "snowbrush",
        "spotify",
        "strftime",
        "strs",
        "struct",
        "systeminformation",
        "testcase",
        "titlebar",
        "touchpad",
        "trackpad",
        "urllib",
        "vastiny",
        "vetur",
        "vim",
        "vuex",
        "webdav",
        "xmind",
        "yantze"
    ],
    "leetcode.defaultLanguage": "javascript",
    "editor.defaultFormatter": null,
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
        // "editor.defaultFormatter": "esbenp.prettier-vscode"
        // "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[xml]": {
        "editor.defaultFormatter": "redhat.vscode-xml"
    },
    "[python]": {},
    "git.ignoreMissingGitWarning": true,
    "breadcrumbs.enabled": true,
    "workbench.editor.showTabs": true,
    "git.ignoreLegacyWarning": true,
    "leetcode.hint.configWebviewMarkdown": false,
    "leetcode.hint.commandShortcut": false,
    "leetcode.workspaceFolder": "/Users/yantze/work/practice/js/demo",
    "leetcode.hint.commentDescription": false,
    "python.linting.pylintArgs": [
        "--extension-pkg-whitelist=cv2"
    ],
    "java.errors.incompleteClasspath.severity": "ignore",
    "C_Cpp.updateChannel": "Insiders",
    "gitlens.statusBar.enabled": false,
    "gitlens.views.repositories.location": "gitlens",
    "gitlens.views.fileHistory.location": "gitlens",
    "gitlens.views.lineHistory.location": "gitlens",
    "gitlens.views.compare.location": "gitlens",
    "gitlens.views.search.location": "gitlens",
    "editor.renderControlCharacters": true,
    "task.problemMatchers.neverPrompt": {
        "npm": true
    },
    "python.jediEnabled": false,
    "javascript.format.insertSpaceBeforeFunctionParenthesis": true, //让函数(名)和后面的括号之间加个空格
    "diffEditor.renderSideBySide": false,
    "aws.profile": "profile:default",
    "aws.samcli.location": "/usr/local/bin/sam",
    // "vetur.format.defaultFormatter.js": "vscode-typescript",
    "vetur.format.defaultFormatter.ts": "vscode-typescript",
    "typescript.updateImportsOnFileMove.enabled": "always",
    // "vetur.format.defaultFormatter.less": "none",
    // "vetur.format.defaultFormatter.html": "js-beautify-html",
}